{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "SOLAR SERVICES UI",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "port": 9229,
            "restart": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "timeout": 30000,
            "skipFiles": [
                "<node_internals>/**"
            ],
        }
    ]
}